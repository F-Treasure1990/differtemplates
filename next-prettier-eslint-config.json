{
  "name": "my-app",
  "type": "registry:block",
  "dependencies": [],
  "devDependencies": [
    "@trivago/prettier-plugin-sort-imports",
    "eslint-config-prettier",
    "eslint-plugin-check-file",
    "eslint-plugin-n",
    "prettier",
    "prettier-plugin-tailwindcss"
  ],
  "registryDependencies": [],
  "files": [
    {
      "path": "./.eslintrc.json",
      "content": "{\n  \"extends\": [\n    \"next/core-web-vitals\",\n    \"next/typescript\",\n    \"prettier\"\n  ],\n  \"plugins\": [\n    \"check-file\",\n    \"n\"\n  ],\n  \"rules\": {\n    \"prefer-arrow-callback\": [\n      \"error\"\n    ],\n    \"prefer-template\": [\n      \"error\"\n    ],\n    \"semi\": [\n      \"error\"\n    ],\n    \"n/no-process-env\": [\n      \"error\"\n    ],\n    \"check-file/filename-naming-convention\": [\n      \"error\",\n      {\n        \"**/*.{ts,tsx}\": \"KEBAB_CASE\"\n      },\n      {\n        \"ignoreMiddleExtensions\": true\n      }\n    ],\n    \"check-file/folder-naming-convention\": [\n      \"error\",\n      {\n        \"src/**/!^[.*\": \"KEBAB_CASE\"\n      }\n    ]\n  }\n}\n",
      "type": "registry:example",
      "target": "~/.eslintrc.json"
    },
    {
      "path": "./.prettierrc.json",
      "content": "{\n  \"semi\": true,\n  \"singleQuote\": true,\n  \"printWidth\": 80,\n  \"useTabs\": false,\n  \"tabWidth\": 2,\n  \"trailingComma\": \"es5\",\n  \"importOrder\": [\n    \"^(react|next?/?([a-zA-Z/]*))$\",\n    \"<THIRD_PARTY_MODULES>\",\n    \"^@/(.*)$\",\n    \"^[./]\"\n  ],\n  \"importOrderSeparation\": true,\n  \"importOrderSortSpecifiers\": true,\n  \"plugins\": [\n    \"@trivago/prettier-plugin-sort-imports\",\n    \"prettier-plugin-tailwindcss\"\n  ]\n}\n",
      "type": "registry:example",
      "target": "~/.prettierrc.json"
    }
  ],
  "tailwind": {},
  "cssVars": {},
  "meta": {}
}
